{
  "name": "PFinalClub",
  "version": "1.0.0",
  "private": true,
  "author": "PFinalClub",
  "license": "MIT",
  "scripts": {
    "dev": "vitepress dev docs",
    "build": "vitepress build docs && node scripts/process-sitemap.mjs && node scripts/copy-redirects.mjs && node scripts/generate-rss.mjs && node scripts/create-clean-url-redirects.mjs",
    "serve": "vitepress serve docs",
    "check:titles": "node scripts/check-titles.mjs docs",
    "fix:titles": "node scripts/check-titles.mjs --fix=quote docs",
    "report:golang-gap": "node scripts/report-golang-translation-gap.mjs",
    "report:golang-gap:zh2en": "node scripts/report-golang-translation-gap.mjs --direction=zh2en --output=en",
    "validate:seo": "node scripts/validate-seo-fixes.mjs",
    "generate:rss": "node scripts/generate-rss.mjs"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@giscus/vue": "^3.0.0",
    "@sugarat/theme": "^0.5.4",
    "element-plus": "^2.7.2",
    "vue": "3.5.12"
  },
  "directories": {
    "doc": "docs"
  },
  "devDependencies": {
    "fast-glob": "^3.3.2",
    "feed": "^4.2.2",
    "gray-matter": "^4.0.3",
    "pagefind": "^1.3.0",
    "sass": "^1.76.0",
    "typescript": "^5.4.5",
    "vitepress": "1.6.4"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "search-insights"
      ]
    }
  }
}
